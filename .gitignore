# Ignore logs
*.log

# Ignore Emacs clutter files
emacs/.emacs.d/*
!emacs/.emacs.d/config.org
!emacs/.emacs.d/early-init.el
!emacs/.emacs.d/init.el
!emacs/.emacs.d/lisp
!emacs/.emacs.d/lisp/*
!emacs/.emacs.d/site-lisp
!emacs/.emacs.d/site-lisp/*
!emacs/.emacs.d/myelpa
!emacs/.emacs.d/myelpa/*
*~undo-tree~*

# Ignore MPD clutter files
mpd/.mpd/*
!mpd/.mpd/mpd.conf
!mpd/.mpd/playlists
!mpd/.mpd/playlists/*

# Ignore MPV clutter files
mpv/.config/mpv/*
!mpv/.config/mpv/input.conf
!mpv/.config/mpv/mpv.conf

# Ignore rofi clutter files
rofi/.local/share/*
rofi/.local/state/*
!rofi/.local/share/rofi/

# Ignore autoload from nvim
nvim/.config/nvim/autoload/*

# Ignore nvim spell files
nvim/.config/nvim/spell/*
!nvim/.config/nvim/spell/en.utf-8.spl
!nvim/.config/nvim/spell/sl.utf-8.spl

# Ignore fish variables file
fish/.config/fish/fish_variables

# Ignore all files in options folder
# Used for temp files or files that toggle some functionality in my scripts
options/*
